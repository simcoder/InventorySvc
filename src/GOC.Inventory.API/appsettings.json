{
  "InventoryService": {
    "Consul": {
      "Host": "consul",
      "Port": 8500,
      "Heartbeat": 10
    },
    "Identity": {
      "Authority": "http://vagrant:5000",
      "ApiName": "api2",
      "ApiSecret": "api2-secret"
    },
    "Rabbit": {
      "Host": "vagrant",
      "PublisherConfirms" : "true",
      "Timeout" : "10",
      "QueueName": "InventoryService_Queue",
      "ExchangeName": "InventoryService_Exchange",
      "RoutingKey": "A.*" 
    }
  },
  "Serilog": {
    "Filter": [
      {
        "Name": "ByExcluding",
        "Args": {
          "expression": "RequestPath = '/status'"
        }
      }
    ],
    "MinimumLevel": "Information",
    "Using": [ "Serilog.Sinks.Literate" ],
    "WriteTo": [
      {
        "Name": "LiterateConsole"
      }
    ]
  },
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Debug"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Debug"
      }
    }
  }
}
